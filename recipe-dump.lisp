Action Definitions
(DEFINE-ACTION BREAK-EGGS-INTO-RECEPTACLE :ARGUMENTS (?RECEPTACLE) :OUTPUTS ((?THE-MIXTURE SET-OF-LIQUIDS EGG-CONTENTS)) :PREREQUISITES
               ([ABOVE ?VICTIM ?RECEPTACLE] [HAS ?ACTOR ?VICTIM] [HAS ?ACTOR ?RECEPTACLE]) :POST-CONDITIONS
               ([IN ?VICTIM.INSIDES ?RECEPTACLE] [BROKEN ?VICTIM.SHELL] [NOT [IN ?VICTIM.INSIDES ?VICTIM.SHELL]] [VALUE-OF (?THE-MIXTURE CONSTITUENT) ?VICTIM.INSIDES.WHITES]
                [VALUE-OF (?THE-MIXTURE CONSTITUENT) ?VICTIM.INSIDES.YOLK])
               :TYPING ((?ACTOR COOK) (?VICTIM EGG) (?RECEPTACLE RECEPTACLE))) 
(DEFINE-ACTION STIR-EGGS :ARGUMENTS (?ACTOR ?THE-EGGS ?RECEPTACLE ?UTENSIL) :OUTPUTS ((?THE-MIXTURE LIQUID-MIXTURE THE-MIXTURE)) :PREREQUISITES
               ([IN ?THE-EGGS ?RECEPTACLE] [HAS ?ACTOR ?RECEPTACLE] [HAS ?ACTOR ?UTENSIL]) :POST-CONDITIONS
               ([CONSTITUENT-OF ?THE-EGGS.YOLK ?THE-MIXTURE] [CONSTITUENT-OF ?THE-EGGS.WHITES ?THE-MIXTURE] [IN ?THE-MIXTURE ?RECEPTACLE]) :TYPING ((?THE-EGGS EGG-CONTENTS))) 
(DEFINE-ACTION FRY-EGGS :ARGUMENTS (?ACTOR ?MIXTURE ?RECEPTACLE ?HEAT-SOURCE) :OUTPUTS NIL :PREREQUISITES
               ([IN ?MIXTURE ?RECEPTACLE] [VALUE-OF ?RECEPTACLE.TEMPERATURE HOT] [VALUE-OF ?HEAT-SOURCE.SWITCH-POSITION HIGH]) :POST-CONDITIONS
               ([STATE-OF ?MIXTURE ?FRIED] [IS-FLEXIBLE ?MIXTURE]) :TYPING NIL) 
(DEFINE-ACTION ADD-INGREDIENT :ARGUMENTS (?ACTOR ?INGREDIENT ?SOMETHING) :OUTPUTS NIL :PREREQUISITES ([HAS ?ACTOR ?INGREDIENT]) :POST-CONDITIONS
               ([CONSTITUENT-OF ?INGREDIENT ?SOMETHING]) :TYPING NIL) 
(DEFINE-ACTION FOLD :ARGUMENTS (?ACTOR ?THING ?INSTRUMENT) :OUTPUTS NIL :PREREQUISITES ([HAS ?ACTOR ?INSTRUMENT] [IS-FLEXIBLE ?THING]) :POST-CONDITIONS ([IS-FOLDED ?THING])
               :TYPING NIL) 